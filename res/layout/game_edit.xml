<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/parent_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Orange"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/framer1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:background="@color/cornsilk"
        android:padding="3dp" >

    </FrameLayout>

    <RelativeLayout
        android:id="@+id/scoreboard_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="12dp" >

        <TextView
            android:id="@+id/lab_points"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="@string/points"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_rebounds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/lab_points"
            android:layout_below="@+id/lab_points"
            android:layout_marginTop="3dp"
            android:text="@string/rebounds"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_assists"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/lab_points"
            android:layout_below="@+id/lab_rebounds"
            android:layout_marginTop="3dp"
            android:text="@string/assists"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_steals"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/lab_points"
            android:layout_below="@+id/lab_assists"
            android:layout_marginTop="3dp"
            android:text="@string/steals"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_blocks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_points"
            android:layout_marginLeft="45dp"
            android:layout_toRightOf="@+id/lab_rebounds"
            android:text="@string/blocks"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_turnovers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_rebounds"
            android:layout_alignLeft="@+id/lab_blocks"
            android:text="@string/turnovers"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_fouls"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_assists"
            android:layout_alignLeft="@+id/lab_blocks"
            android:text="@string/fouls"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_minutes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_steals"
            android:layout_alignLeft="@+id/lab_blocks"
            android:text="@string/minutes"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_fgs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_points"
            android:layout_marginLeft="45dp"
            android:layout_toRightOf="@+id/lab_turnovers"
            android:text="@string/fgs"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_fgp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_turnovers"
            android:layout_alignLeft="@+id/lab_fgs"
            android:text="@string/fg3s"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_fg3ma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_steals"
            android:layout_alignLeft="@+id/lab_fgs"
            android:text="@string/fts"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_points"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_points"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/lab_rebounds"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_rebounds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_rebounds"
            android:layout_alignBottom="@+id/lab_turnovers"
            android:layout_alignLeft="@+id/dis_points"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_assists"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_assists"
            android:layout_alignLeft="@+id/dis_points"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_steals"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_steals"
            android:layout_alignLeft="@+id/dis_points"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_blocks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_points"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/lab_turnovers"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_turnovers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_rebounds"
            android:layout_alignLeft="@+id/dis_blocks"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_fouls"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_assists"
            android:layout_alignLeft="@+id/dis_blocks"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_minutes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_steals"
            android:layout_alignLeft="@+id/dis_blocks"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_fgs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_points"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/lab_fgs"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_fg3s"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_rebounds"
            android:layout_alignLeft="@+id/dis_fgs"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_fts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_steals"
            android:layout_alignLeft="@+id/dis_fgs"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/lab_fg2ma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_assists"
            android:layout_alignLeft="@+id/lab_fgs"
            android:text="@string/fg2s"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dis_fg2s"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lab_assists"
            android:layout_alignLeft="@+id/dis_fgs"
            android:text="@string/zero"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </RelativeLayout>

    <ScrollView
        android:id="@+id/child_container2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@color/WhiteSmoke"
        android:paddingBottom="15dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="15dp"
        android:scrollbars="vertical|horizontal" >

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout01"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_fg2made"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />
                    </FrameLayout>

                    <Button
                        android:id="@+id/press_fg2made"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout01"
                        android:minHeight="24dp"
                        android:text="@string/twos_made"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/box_fg2made"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_fg2missed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />
                    </FrameLayout>

                    <Button
                        android:id="@+id/press_fg2missed"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/box_fg2made"
                        android:minHeight="24dp"
                        android:text="@string/twos_missed"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_fg3made"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />
                    </FrameLayout>

                    <Button
                        android:id="@+id/press_fg3made"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout03"
                        android:minHeight="24dp"
                        android:text="@string/threes_made"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_fg3missed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />
                    </FrameLayout>

                    <Button
                        android:id="@+id/press_fg3missed"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout02"
                        android:minHeight="24dp"
                        android:text="@string/threes_missed"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>
            </TableRow>

            <TableRow
                android:id="@+id/TableRow01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout07"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_ftmade"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                    </FrameLayout>

                    <Button
                        android:id="@+id/press_ftmade"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout07"
                        android:minHeight="24dp"
                        android:text="@string/ft_made"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout06"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_ftmissed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                    </FrameLayout>

                    <Button
                        android:id="@+id/press_ftmissed"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout06"
                        android:minHeight="24dp"
                        android:text="@string/ft_miss"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout04"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_reboff"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                    </FrameLayout>

                    <Button
                        android:id="@+id/press_reboff"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout04"
                        android:minHeight="24dp"
                        android:text="@string/reboff"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout05"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_rebdef"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                    </FrameLayout>

                    <Button
                        android:id="@+id/press_rebdef"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout05"
                        android:minHeight="24dp"
                        android:text="@string/rebdef"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>
            </TableRow>

            <TableRow
                android:id="@+id/TableRow02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout04"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_assists"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                    </FrameLayout>

                    <Button
                        android:id="@+id/press_assists"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout04"
                        android:minHeight="24dp"
                        android:text="@string/ast"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout05"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_steals"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                    </FrameLayout>

                    <Button
                        android:id="@+id/press_steals"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout05"
                        android:minHeight="24dp"
                        android:text="@string/stl"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>
                
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout08"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_blocks"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />
                    </FrameLayout>

                    <Button
                        android:id="@+id/press_blocks"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout08"
                        android:minHeight="24dp"
                        android:text="@string/blk"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout09"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" >

                        <TextView
                            android:id="@+id/stat_turnovers"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:maxLength="2"
                            android:maxLines="1"
                            android:text="@string/double_zero"
                            android:textAppearance="?android:attr/textAppearanceLarge" />
                    </FrameLayout>

                    <Button
                        android:id="@+id/press_turnovers"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/FrameLayout09"
                        android:minHeight="24dp"
                        android:text="@string/tos"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </RelativeLayout>
            </TableRow>

        </TableLayout>

    </ScrollView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/LightGrey"
        android:padding="10dp" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center_horizontal" >

            <Button
                android:id="@+id/minus1"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/minus1" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|center_horizontal"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@color/White"
                android:padding="8dp" >

                <SeekBar
                    android:id="@+id/game_seek"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </FrameLayout>

            <Button
                android:id="@+id/plus1"
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/plus1" />
        </TableRow>
    </RelativeLayout>

    <FrameLayout
        android:id="@+id/relativebottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="2dp"
        android:layout_marginTop="7dp" >

        <TableRow
            android:id="@+id/child_container3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/reset_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:drawable/btn_default"
                android:drawableLeft="@drawable/ic_button_reset"
                android:onClick="resetStats"
                android:text="@string/reset" />

            <Button
                android:id="@+id/save_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:drawable/btn_default"
                android:drawableLeft="@drawable/ic_menu_done"
                android:onClick="saveStats"
                android:text="@string/save" />
        </TableRow>
    </FrameLayout>

</LinearLayout>